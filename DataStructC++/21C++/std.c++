#include <iostream>

int main() {
    // 使用C++标准输出流输出多行文本
    std::cout << "First line" << std::endl;
    std::cout << "Second line" << std::endl;
    std::cout << "Third line" << std::endl;
    
    // 使用C++标准输出流输出数字
    int number = 42;
    std::cout << "The number is: " << number << std::endl;
    
    // 使用C++标准输出流输出多个变量
    double pi = 3.14159;
    std::cout << "Pi is approximately: " << pi << std::endl;
    
    return 0;
}
